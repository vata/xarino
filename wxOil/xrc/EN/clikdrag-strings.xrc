				<object class="wxStaticText" name="IDS_TRANSLTRANS_STATUS1">
					<label>Drag to move objects</label>
				</object>
			</object>
			<object class="sizeritem">
				<flag>wxALIGN_LEFT|wxALL|wxADJUST_MINSIZE</flag>
				<object class="wxStaticText" name="IDS_TRANSLTRANS_STATUS2">
					<label>; Ctrl to constrain direction</label>
				</object>
			</object>
		</object>
